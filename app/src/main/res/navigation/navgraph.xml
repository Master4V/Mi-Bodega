<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navgraph"
    app:startDestination="@id/inicioFragment">

    <fragment
        android:id="@+id/inicioFragment"
        android:name="com.example.mibodega.InicioFragment"
        android:label="fragment_inicio"
        tools:layout="@layout/fragment_inicio" >
        <action
            android:id="@+id/action_inicioFragment_to_verpag2Fragment"
            app:destination="@id/verpag2Fragment"
            app:popUpTo="@id/inicioFragment"
            app:popUpToInclusive="false" />
    </fragment>
    <fragment
        android:id="@+id/verpag2Fragment"
        android:name="com.example.mibodega.verpag2Fragment"
        android:label="fragment_verpag2"
        tools:layout="@layout/fragment_verpag2" >
        <argument
            android:name="nombre"
            app:argType="string" />
        <action
            android:id="@+id/action_verpag2Fragment_to_agarraFragment"
            app:destination="@id/agarraFragment"
            app:popUpTo="@id/verpag2Fragment"
            app:popUpToInclusive="false" />
        <action
            android:id="@+id/action_verpag2Fragment_to_inicioFragment"
            app:destination="@id/inicioFragment" />
    </fragment>
    <fragment
        android:id="@+id/creditFragment"
        android:name="com.example.mibodega.CreditFragment"
        android:label="fragment_credit"
        tools:layout="@layout/fragment_credit" >
        <argument
            android:name="nombre"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/menuFragment"
        android:name="com.example.mibodega.MenuFragment"
        android:label="fragment_menu"
        tools:layout="@layout/fragment_menu" >
        <argument
            android:name="nombre"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/agarraFragment"
        android:name="com.example.mibodega.AgarraFragment"
        android:label="fragment_agarra"
        tools:layout="@layout/fragment_agarra" >
        <argument
            android:name="nombre"
            app:argType="string" />
    </fragment>
</navigation>